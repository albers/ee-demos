<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
  <flow-definition id="flow2">
    <start-node>flow2a</start-node>
    <view id="flow2a">
      <vdl-document>/flow2/flow2a.xhtml</vdl-document>
    </view>
    <flow-return id="exitFlow">
      <from-outcome>exitFlow</from-outcome>
    </flow-return>
    <navigation-rule>
      <from-view-id>/flow2/flow2a.xhtml</from-view-id>
      <navigation-case>
        <from-outcome>next</from-outcome>
        <to-view-id>/flow2/flow2b.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <from-view-id>/flow2/flow2b.xhtml</from-view-id>
      <navigation-case>
        <from-outcome>prev</from-outcome>
        <to-view-id>/flow2/flow2a.xhtml</to-view-id>
      </navigation-case>
    </navigation-rule>
  </flow-definition>
  <application />
</faces-config>
